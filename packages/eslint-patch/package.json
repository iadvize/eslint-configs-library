{
  "name": "@iadvize-oss/eslint-patch",
  "version": "0.0.2",
  "description": "Workaround for eslint plugin resolution",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iadvize/eslint-configs-library.git"
  },
  "keywords": [
    "iAdvize"
  ],
  "author": "iAdvize developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iadvize/eslint-configs-library/issues"
  },
  "homepage": "https://github.com/iadvize/eslint-configs-library#readme",
  "devDependencies": {
    "@iadvize-oss/eslint-config": "0.1.0",
    "@jarrodldavis/remark-changelog-version-bump": "^0.1.1",
    "eslint": "^6.8.0",
    "remark-cli": "^7.0.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "eslint": "^6.8.0"
  },
  "scripts": {
    "lint:js": "eslint *.js",
    "bump-changelog": "VERSION=$(cat package.json | jq '.version') && remark CHANGELOG.md -o --use @jarrodldavis/changelog-version-bump=version:$VERSION && git add CHANGELOG.md"
  }
}
