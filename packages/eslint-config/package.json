{
  "name": "@iadvize-oss/eslint-config",
  "version": "0.0.1-beta.0",
  "description": "iAdvize javascript eslint rules, based on airbnb",
  "main": "index.js",
  "author": "iAdvize developers",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/iadvize/eslint-configs-library"
  },
  "devDependencies": {
    "@jarrodldavis/remark-changelog-version-bump": "^0.1.1",
    "eslint": "^6.8.0",
    "remark-cli": "^7.0.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "typescript": "^3.4.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-tsdoc": "^0.2.1",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "lint:js": "eslint *.js",
    "bump-changelog": "VERSION=$(cat package.json | jq '.version') && remark CHANGELOG.md -o --use @jarrodldavis/changelog-version-bump=version:$VERSION && git add CHANGELOG.md"
  }
}
